<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <!-- TODO: Update blank fragment layout -->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.v7.widget.CardView
                android:id="@+id/cardView"
                android:layout_width="326dp"
                android:layout_height="206dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                app:cardCornerRadius="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.39"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="12dp"
                    android:text="500 meters from you"
                    tools:layout_conversion_absoluteHeight="19dp"
                    tools:layout_conversion_absoluteWidth="133dp"
                    tools:layout_editor_absoluteX="52dp"
                    tools:layout_editor_absoluteY="28dp" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="137dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/roboto_condensed_bold"
                    android:text="Sells:
                    \n ✓ Bawang Putih \n ✓ Bawang Merah \n ✓ Beras \n ✓ Daging Ayam \n ✓ Kecap Asin "
                    android:textColor="#333333" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="40dp"
                    android:src="@drawable/kios1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="135dp"
                    android:fontFamily="@font/roboto_condensed_bold"
                    android:text="Warung Sayur Bude Lastri"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="160dp"
                    android:fontFamily="@font/roboto_condensed_light"
                    android:text="Pogung Dalangan 410, Yogyakarta \nContact: 08128976887"
                    android:textColor="#333333" />

                <Button
                    android:id="@+id/shop_now"
                    android:layout_width="80dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginTop="145dp"
                    android:background="#6A5222"
                    android:fontFamily="@font/roboto_condensed_light"
                    android:text="Shop here"
                    android:textAllCaps="false"
                    android:textColor="#fff"
                    android:textStyle="bold" />

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardView1"
                android:layout_width="326dp"
                android:layout_height="206dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                app:cardCornerRadius="15dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.39"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView">

                <TextView
                    android:id="@+id/textViewa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="12dp"
                    android:text="950 meters from you"
                    tools:layout_conversion_absoluteHeight="19dp"
                    tools:layout_conversion_absoluteWidth="133dp"
                    tools:layout_editor_absoluteX="52dp"
                    tools:layout_editor_absoluteY="28dp" />

                <TextView
                    android:id="@+id/textViewb"
                    android:layout_width="137dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/roboto_condensed_bold"
                    android:text="Sells:
                    \n ✓ Bawang Putih \n ✓ Bawang Merah \n ✓ Daging Ayam  "
                    android:textColor="#333333" />

                <ImageView
                    android:id="@+id/imageViewc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="40dp"
                    android:src="@drawable/kios2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="135dp"
                    android:fontFamily="@font/roboto_condensed_bold"
                    android:text="Warung MieMie 99"
                    android:textColor="#333333"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="160dp"
                    android:fontFamily="@font/roboto_condensed_light"
                    android:text="Monjali 45, Sleman \nContact: 08199591191"
                    android:textColor="#333333" />

                <Button
                    android:layout_width="80dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginTop="145dp"
                    android:background="#6A5222"
                    android:fontFamily="@font/roboto_condensed_light"
                    android:text="Shop here"
                    android:textAllCaps="false"
                    android:textColor="#fff"
                    android:textStyle="bold" />
            </android.support.v7.widget.CardView>

        </android.support.constraint.ConstraintLayout>

    </ScrollView>
</android.support.constraint.ConstraintLayout>